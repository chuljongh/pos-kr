{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/cafe/kiosk-solution.html", // 👈 새로 없어진 페이지 주소
      "destination": "/", // 👈 메인 페이지 (pos.kr/)로 영구히 안내
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.pos.kr"
        }
      ],
      "destination": "https://pos.kr/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/cafe",
      "destination": "/cafe/index.html"
    },
    {
      "source": "/hamba",
      "destination": "/hamba/index.html"
    }
  ]
}