{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.pos.kr"
        }
      ],
      "destination": "https://pos.kr/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/cafe",
      "destination": "/cafe/index.html"
    },
    {
      "source": "/hamba",
      "destination": "/hamba/index.html"
    }
  ]
}