{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.pos.kr"
        }
      ],
      "destination": "https://pos.kr/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/cafe",
      "destination": "/cafe/index.html"
    },
    {
      "source": "/hamba",
      "destination": "/hamba/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\.(ico|svg|jpg|jpeg|png|gif|webp|js|css|woff|woff2|eot|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, must-revalidate"
        }
      ]
    }
  ]
}